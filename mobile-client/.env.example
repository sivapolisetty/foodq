# FoodQ Mobile App Environment Configuration
# Copy this file to .env and fill in your actual values

# API Configuration
# Production: https://api.foodqapp.com (no /api prefix needed)
# QA: https://foodq.pages.dev/api (requires /api prefix)
API_BASE_URL=https://api.foodqapp.com

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google Maps API Key
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Stripe Configuration
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# Environment
ENVIRONMENT=development

# Debug Settings
DEBUG_API_CALLS=true
DEBUG_LOCATION=true

# Optional: Override API endpoints for local development
# API_BASE_URL=http://localhost:8788

# Environment-specific API URLs:
# QA/Testing: https://foodq.pages.dev/api
# Production: https://api.foodqapp.com
# Local Dev: http://localhost:8788